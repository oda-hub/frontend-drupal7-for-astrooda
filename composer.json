	"require" : {
		"composer/installers": "^2.0",
		"cweagans/composer-patches": "^1.7",
		"datatables/datatables-buttons": "^3.2",
		"datatables/datatables-buttons-bs5": "^3.2",
		"datatables/datatables-dist": "^2.1",
		"datatables/datatables-dist-bs5": "^2.1",
		"datatables/datatables-select": "^2.1",
		"datatables/datatables-select-bs5": "^2.1",
		"drupal/admin_toolbar": "^3.4",
		"drupal/ajax_login_popup": "^2.0",
		"drupal/book": "^2.0@alpha",
		"drupal/bootstrap5": "^4.0@alpha",
		"drupal/ckeditor_colorbox_inline": "^1.2",
		"drupal/colorbox": "^2.1",
		"drupal/colorbox_inline": "^2.0",
		"drupal/content_sync": "4.0.x-dev",
		"drupal/core-composer-scaffold": "^10.1",
		"drupal/core-project-message": "^10.1",
		"drupal/core-recommended": "^10.1",
		"drupal/ctools": "^4.0",
		"drupal/devel": "^5.1",
		"drupal/eme": "^1.0@alpha",
		"drupal/entity_content_export": "^2.0@alpha",
		"drupal/eu_cookie_compliance": "^1.24",
		"drupal/examples": "^4.0",
		"drupal/highlight_js": "^1.1",
		"drupal/jquery_ui": "^1.6",
		"drupal/jquery_ui_draggable": "^2.0",
		"drupal/jquery_ui_effects": "^2.0",
		"drupal/jquery_ui_resizable": "^2.0",
		"drupal/jwt": "^2.1",
		"drupal/libraries": "^4.0",
		"drupal/mimemail": "^1.0@alpha",
		"drupal/node_export": "^1.0",
		"drupal/openid_connect": "^3.0@alpha",
		"drupal/pathauto": "^1.12",
		"drupal/redirect_after_login": "^3.0",
		"drupal/remove_unused_files": "^2.1",
		"drupal/twig_tweak": "^3.4",
		"drupal/views_bulk_operations": "^4.2",
		"drupal/views_slideshow": "^5.0",
		"drupal/webform": "^6.2",
		"drush/drush": "^12.5",
		"jackmoore/colorbox": "^1.6",
		"marioizquierdo/jquery.serialize-json": "^3.2",
		"oda/bootstrap_mmoda": "dev-d10-main",
		"oda/datatables-editor": "^2.4",
		"oda/form-validation": "^2.4",
		"oda/mmoda": "dev-d10-main"
	},
	"conflict" : {
		"drupal/drupal" : "*"
	},
	"minimum-stability" : "stable",
	"prefer-stable" : true,
	"config" : {
		"allow-plugins" : {
			"composer/installers" : true,
			"drupal/core-composer-scaffold" : true,
			"drupal/core-project-message" : true,
			"phpstan/extension-installer" : true,
			"dealerdirect/phpcodesniffer-composer-installer" : true,
			"cweagans/composer-patches" : true
		},
		"sort-packages" : true
	},
	"extra" : {
		"drupal-scaffold" : {
			"locations" : {
				"web-root" : "web/"
			},
			"file-mapping" : {
				"[web-root]/sites/default.services.yml" : false,
				"[web-root]/sites/default.settings.php" : false,
				"[web-root]/sites/development.services.yml" : false
			}
		},
		"patches" : {
			"drupal/openid_connect" : {
				"Allow  disconnect users" : "https://www.drupal.org/files/issues/2023-08-31/openid_connect-cannot_disconnect_users-3298784-3.patch"
			}
		},
		"installer-paths" : {
			"web/core" : [
				"type:drupal-core"
			],
			"web/libraries/{$name}" : [
				"type:drupal-library"
			],
			"web/modules/contrib/{$name}" : [
				"type:drupal-module"
			],
			"web/profiles/contrib/{$name}" : [
				"type:drupal-profile"
			],
			"web/themes/contrib/{$name}" : [
				"type:drupal-theme"
			],
			"drush/Commands/contrib/{$name}" : [
				"type:drupal-drush"
			],
			"web/modules/custom/{$name}" : [
				"type:drupal-custom-module"
			],
			"web/profiles/custom/{$name}" : [
				"type:drupal-custom-profile"
			],
			"web/themes/custom/{$name}" : [
				"type:drupal-custom-theme"
			]
		},
		"drupal-core-project-message" : {
			"include-keys" : [
				"homepage",
				"support"
			],
			"post-create-project-cmd-message" : [
				"<bg=blue;fg=white>                                                         </>",
				"<bg=blue;fg=white>  Congratulations, you\u2019ve installed the Drupal codebase  </>",
				"<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
				"<bg=blue;fg=white>                                                         </>",
				"",
				"<bg=yellow;fg=black>Next steps</>:",
				"  * Install the site: https://www.drupal.org/docs/installing-drupal",
				"  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
				"  * Get support: https://www.drupal.org/support",
				"  * Get involved with the Drupal community:",
				"      https://www.drupal.org/getting-involved",
				"  * Remove the plugin that prints this message:",
				"      composer remove drupal/core-project-message"
			]
		}
	}
}
