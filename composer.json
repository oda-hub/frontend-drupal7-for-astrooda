				"version" : "2.1.8",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@github.com:DataTables/Dist-DataTables.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "datatables/datatables-dist-bs5",
				"version" : "2.1.8",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@github.com:DataTables/Dist-DataTables-Bootstrap5.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "datatables/datatables-buttons",
				"version" : "3.2.0",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@github.com:DataTables/Dist-DataTables-Buttons.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "datatables/datatables-buttons-bs5",
				"version" : "3.2.0",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@github.com:DataTables/Dist-DataTables-Buttons-Bootstrap5.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "datatables/datatables-select",
				"version" : "2.1.0",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@github.com:DataTables/Dist-DataTables-Select.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "datatables/datatables-select-bs5",
				"version" : "2.1.0",
				"type" : "drupal-library",
