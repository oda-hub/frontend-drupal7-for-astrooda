				"name" : "oda/form-validation",
				"version" : "2.4.0",
				"type" : "drupal-library",
				"source" : {
					"url" : "git@gitlab.astro.unige.ch:oda/form-validation.git",
					"type" : "git",
					"reference" : "main"
