				"name" : "jackmoore/colorbox",
				"version" : "1.6.4",
				"type" : "drupal-library",
				"dist" : {
					"url" : "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
					"type" : "zip"
				}
			}
		}, {
			"type" : "package",
			"package" : {
				"name" : "marioizquierdo/jquery.serialize-json",
				"version" : "3.2.1",
				"type" : "drupal-library",
				"source" : {
					"url" : "https://github.com/marioizquierdo/jquery.serializeJSON.git",
					"type" : "git",
					"reference" : "master"
				}
			}
		}, {
